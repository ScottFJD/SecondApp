<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:background="@color/bg_white"
    android:id="@+id/cl_physical_forth"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/iv_physical_post"
        android:layout_width="110dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/shopkeeper_uploadimg"
        app:layout_constraintRight_toLeftOf="@+id/iv_physical_back"
        app:layout_constraintLeft_toRightOf="@+id/iv_physical_positive"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" />

    <ImageView
        android:id="@+id/iv_physical_back"
        android:layout_width="110dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/shopkeeper_uploadimg"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/iv_physical_post"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" />

    <ImageView
        android:id="@+id/iv_physical_tongue"
        android:layout_width="110dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/shopkeeper_uploadimg"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/iv_physical_positive"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/iv_physical_positive"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView86" />

    <ImageView
        android:id="@+id/iv_physical_positive"
        android:layout_width="110dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/shopkeeper_uploadimg"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/iv_physical_post"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" />

    <TextView
        android:id="@+id/textView86"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="正面"
        android:textColor="@color/text_color"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/iv_physical_positive"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/iv_physical_positive"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/iv_physical_positive" />

    <TextView
        android:id="@+id/textView108"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="侧面"
        android:textColor="@color/text_color"
        app:layout_constraintLeft_toLeftOf="@+id/iv_physical_post"
        app:layout_constraintRight_toRightOf="@+id/iv_physical_post"
        app:layout_constraintTop_toBottomOf="@+id/iv_physical_post" />

    <TextView
        android:id="@+id/textView109"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="背面"
        android:textColor="@color/text_color"
        app:layout_constraintLeft_toLeftOf="@+id/iv_physical_back"
        app:layout_constraintRight_toRightOf="@+id/iv_physical_back"
        app:layout_constraintTop_toBottomOf="@+id/iv_physical_back" />

    <TextView
        android:id="@+id/textView110"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="舌苔"
        android:textColor="@color/text_color"
        app:layout_constraintLeft_toLeftOf="@+id/iv_physical_tongue"
        app:layout_constraintRight_toRightOf="@+id/iv_physical_tongue"
        app:layout_constraintTop_toBottomOf="@+id/iv_physical_tongue" />

    <ImageView
        android:id="@+id/iv_shoppker_delete_1"
        android:layout_width="21dp"
        android:layout_height="21dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:visibility="gone"
        app:layout_constraintRight_toRightOf="@+id/iv_physical_positive"
        app:layout_constraintTop_toTopOf="@+id/iv_physical_positive"
        app:srcCompat="@drawable/shoppker_delete" />

    <ImageView
        android:id="@+id/iv_shoppker_delete_4"
        android:layout_width="21dp"
        android:layout_height="21dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:visibility="gone"
        app:layout_constraintRight_toRightOf="@+id/iv_physical_tongue"
        app:layout_constraintTop_toTopOf="@+id/iv_physical_tongue"
        app:srcCompat="@drawable/shoppker_delete" />

    <ImageView
        android:id="@+id/iv_shoppker_delete_3"
        android:layout_width="21dp"
        android:layout_height="21dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:visibility="gone"
        app:layout_constraintRight_toRightOf="@+id/iv_physical_back"
        app:layout_constraintTop_toTopOf="@+id/iv_physical_back"
        app:srcCompat="@drawable/shoppker_delete" />

    <ImageView
        android:id="@+id/iv_shoppker_delete_2"
        android:layout_width="21dp"
        android:layout_height="21dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:visibility="gone"
        app:layout_constraintRight_toRightOf="@+id/iv_physical_post"
        app:layout_constraintTop_toTopOf="@+id/iv_physical_post"
        app:srcCompat="@drawable/shoppker_delete" />
</android.support.constraint.ConstraintLayout>