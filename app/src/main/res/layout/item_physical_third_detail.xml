<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:id="@+id/cl_third_detail"
    android:layout_height="match_parent">

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline2"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="192dp" />

    <TextView
        android:id="@+id/textView71"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:text="运动3min后心率"
        android:textColor="@color/text_color"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_physical_third_6"
        app:layout_constraintRight_toLeftOf="@+id/guideline2" />

    <TextView
        android:id="@+id/textView91"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:text="运动2min后心率"
        android:textColor="@color/text_color"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_physical_third_5"
        app:layout_constraintRight_toLeftOf="@+id/guideline2" />

    <TextView
        android:id="@+id/textView90"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:text="运动1min后心率"
        android:textColor="@color/text_color"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_physical_third_4"
        app:layout_constraintRight_toLeftOf="@+id/guideline2" />

    <TextView
        android:id="@+id/textView87"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="0dp"
        android:text="体前躯"
        android:textColor="@color/text_color"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_physical_third_3"
        app:layout_constraintRight_toRightOf="@+id/textView83" />

    <TextView
        android:id="@+id/tv_third_first_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="上肢力量"
        android:textColor="@color/text_color"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_physical_third_1"
        app:layout_constraintRight_toLeftOf="@+id/guideline2" />

    <TextView
        android:id="@+id/textView83"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="0dp"
        android:text="下肢力量"
        android:textColor="@color/text_color"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_physical_third_2"
        app:layout_constraintRight_toRightOf="@+id/tv_third_first_name" />

    <EditText
        android:paddingLeft="5dp"
        android:id="@+id/et_physical_third_6"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:background="@drawable/physical_content_shape"
        android:inputType="number"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/et_physical_third_5"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline2" />

    <EditText
        android:id="@+id/et_physical_third_5"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:background="@drawable/physical_content_shape"
