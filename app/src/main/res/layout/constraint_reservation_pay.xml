<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:background="@color/bg_grey"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:id="@+id/imageView56"
        android:layout_width="0dp"
        android:layout_height="50dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@color/bg_white" />

    <TextView
        android:id="@+id/textView140"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cang_reservation_pay"
        android:textColor="@color/text_color"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView56"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/tv_reservation_price"
        app:layout_constraintTop_toBottomOf="@+id/imageView56" />

    <TextView
        android:id="@+id/tv_reservation_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="￥00"
        android:textColor="#ff5b45"
        android:textSize="16sp"
        app:layout_constraintLeft_toRightOf="@+id/textView140"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView140"
        android:layout_marginStart="8dp" />

    <ImageView
        android:id="@+id/imageView64"
        android:layout_width="0dp"
        android:layout_height="50dp"
        app:srcCompat="@color/bg_white"
        android:layout_marginTop="1dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView56"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/textView146"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:text="@string/cang_reservation_pay1"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView64"
        app:layout_constraintLeft_toLeftOf="@+id/imageView64"
        app:layout_constraintTop_toTopOf="@+id/imageView64"
        app:layout_constraintVertical_bias="0.515"
        android:layout_marginStart="16dp" />

    <TextView
        android:id="@+id/tv_reservation_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:text="30分钟"
        android:textColor="@color/text_color"
        android:textSize="16sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView146"
        app:layout_constraintLeft_toRightOf="@+id/textView146"
        android:layout_marginStart="8dp" />

    <ImageView
        android:id="@+id/imageView65"
        android:layout_width="0dp"
        android:layout_height="50dp"
        app:srcCompat="@color/bg_white"
        android:layout_marginTop="1dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView64"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/textView149"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:text="@string/cang_reservation_pay2"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView65"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView65"
        android:layout_marginStart="16dp" />

    <TextView
        android:id="@+id/tv_order_code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:text="10000100010010"
        android:textColor="@color/text_color"
        android:textSize="16sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView149"
        app:layout_constraintLeft_toRightOf="@+id/textView149"
        android:layout_marginStart="8dp" />

    <ImageView
        android:id="@+id/imageView66"
        android:layout_width="0dp"
        android:layout_height="50dp"
        app:srcCompat="@color/bg_white"
        app:layout_constraintTop_toBottomOf="@+id/imageView65"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="1.0" />

    <TextView
        android:id="@+id/textView151"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:text="@string/cang_reservation_pay3"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView66"
        app:layout_constraintLeft_toLeftOf="@+id/imageView66"
        app:layout_constraintTop_toTopOf="@+id/imageView66"
        app:layout_constraintVertical_bias="0.515"
        android:layout_marginStart="16dp" />

    <TextView
        android:id="@+id/tv_cang_code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:text="SH0021"
        android:textColor="@color/text_color"
        android:textSize="16sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView151"
        app:layout_constraintLeft_toRightOf="@+id/textView151"
        android:layout_marginStart="8dp" />


    <ImageView
        android:id="@+id/imageView37"
        android:layout_width="0dp"
        android:layout_height="50dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@color/bg_white"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView66" />

    <TextView
        android:id="@+id/textView136"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="选择支付方式"
        android:textSize="17sp"
        android:textColor="@color/text_color"
        app:layout_constraintTop_toTopOf="@+id/imageView37"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView37"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageView37"
        android:layout_marginStart="16dp" />

    <ImageView
        android:id="@+id/imageView38"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginTop="1dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView37"
        app:srcCompat="@color/bg_white" />

    <ImageView
        android:id="@+id/imageView40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/buy_wechat"
        app:layout_constraintTop_toTopOf="@+id/imageView38"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView38"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageView38"
        android:layout_marginStart="16dp" />

    <CheckBox
        android:id="@+id/cb_wechat_pay"
        style="@style/DepositCheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView38"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView38" />

    <ImageView
        android:id="@+id/imageView43"
        android:layout_width="0dp"
        android:layout_height="50dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@color/bg_white"
        android:layout_marginTop="1dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView38"
        app:layout_constraintHorizontal_bias="0.0" />

    <ImageView
        android:id="@+id/imageView44"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/buy_alipay"
        app:layout_constraintTop_toTopOf="@+id/imageView43"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView43"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageView43"
        android:layout_marginStart="16dp" />

    <CheckBox
        android:id="@+id/cb_alipay"
        style="@style/DepositCheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView43"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView43" />

    <TextView
        android:id="@+id/tv_pay"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@color/colorPrimary"
        android:gravity="center"
        android:text="立即支付"
        android:textColor="@color/text_color"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/textView161"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:text="微信"
        android:textColor="@color/text_color"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView38"
        app:layout_constraintLeft_toRightOf="@+id/imageView40"
        app:layout_constraintTop_toBottomOf="@+id/imageView38"
        app:layout_constraintVertical_bias="0.492" />

    <TextView
        android:id="@+id/textView162"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:text="支付宝"
        android:textColor="@color/text_color"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView43"
        app:layout_constraintLeft_toRightOf="@+id/imageView44"
        app:layout_constraintTop_toBottomOf="@+id/imageView43"
        app:layout_constraintVertical_bias="0.492" />


</android.support.constraint.ConstraintLayout>