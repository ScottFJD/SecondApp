<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView122"
        android:layout_width="0dp"
        android:layout_height="200dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/high_bg_training" />

    <com.scottfu.sflibrary.customview.CircleImageView
        android:id="@+id/circleImageView"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginEnd="15dp"
        android:layout_marginTop="35dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView122"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/y_you" />

    <TextView
        android:id="@+id/textView186"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="高级健身教练"
        android:textColor="@color/colorPrimary"
        android:textSize="15sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tv_teach_name"
        app:layout_constraintStart_toEndOf="@+id/tv_teach_name" />

    <TextView
        android:id="@+id/tv_teach_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="35dp"
        android:text="XX/"
        android:textColor="@color/colorPrimary"
        android:textSize="17sp"
        app:layout_constraintStart_toStartOf="@+id/imageView122"
        app:layout_constraintTop_toTopOf="@+id/imageView122" />
</android.support.constraint.ConstraintLayout>