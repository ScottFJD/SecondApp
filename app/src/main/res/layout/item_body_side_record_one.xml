        android:drawableLeft="@drawable/my_health_record_oval_shape"
        android:drawablePadding="10dp"
        android:singleLine="true"
        android:text="血      压"
        android:textColor="@color/text_color"
        app:layout_constraintBaseline_toBaselineOf="@+id/tv_body_side_one_1"
        app:layout_constraintLeft_toLeftOf="@+id/guideline8" />

    <TextView
        android:id="@+id/textView128"
        android:layout_width="76dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"

        android:drawableLeft="@drawable/my_health_record_oval_shape"
        android:drawablePadding="10dp"
        android:singleLine="true"
        android:text="体      重"
        android:textColor="@color/text_color"
        app:layout_constraintBaseline_toBaselineOf="@+id/tv_body_side_one_2"
        app:layout_constraintLeft_toLeftOf="@+id/guideline8" />

    <TextView
        android:id="@+id/textView129"
        android:layout_width="76dp"
        android:layout_height="wrap_content"
