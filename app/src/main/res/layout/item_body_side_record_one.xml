        app:layout_constraintTop_toBottomOf="@+id/textView123" />

    <TextView
        android:id="@+id/textView62"
        android:layout_width="70dp"
        android:layout_height="25dp"
        android:layout_marginLeft="0dp"
        android:background="@drawable/body_side_record_title_shap"
        android:gravity="center"
        android:text="第一节"
        android:textColor="@color/text_color"
        app:layout_constraintBottom_toTopOf="@+id/view46"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.533" />

    <View
        android:id="@+id/view46"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginTop="40dp"
        android:background="@color/bg_grey"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView63"
        android:layout_width="76dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:drawableLeft="@drawable/my_health_record_oval_shape"
        android:drawablePadding="10dp"
        android:singleLine="true"
