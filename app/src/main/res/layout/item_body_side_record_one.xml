        android:drawableLeft="@drawable/my_health_record_oval_shape"
        android:drawablePadding="10dp"
        android:text="身体组成分析报告"
        android:textColor="@color/text_color"
        android:textSize="12sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tv_body_side_one_3"
        app:layout_constraintLeft_toLeftOf="@+id/guideline8" />

    <TextView
        android:id="@+id/tv_body_side_one_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:text="70"
        android:textColor="@color/text_color"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView63"
        app:layout_constraintLeft_toRightOf="@+id/textView63" />

    <TextView
        android:id="@+id/tv_body_side_one_2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:text="70"
        android:textColor="@color/text_color"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView123"
        app:layout_constraintLeft_toRightOf="@+id/textView123" />

    <TextView
        android:id="@+id/tv_body_side_one_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:text="70"
        android:textColor="@color/text_color"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView127"
        app:layout_constraintLeft_toRightOf="@+id/textView127" />

    <TextView
        android:id="@+id/tv_body_side_one_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
